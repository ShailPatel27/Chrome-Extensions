{
    "manifest_version": 3,
    "name": "Budget Manager",
    "version": "1.0",
    "description": "This Extension Manages All Your Budgets",
    "permissions": [
        "storage"
    ],
    "options_page": "options.html",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action":{
        "default_icon":"icon16.png",
        "default_popup":"popup.html"
    }
}